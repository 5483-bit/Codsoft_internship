    "n=int(input(\"What's the length of the password\"))\n",
    "ch=\"\"\n",
    "for i in range (n):\n",
    "    k=random.randint(65,122)\n",
    "    if 91<=k<=96:\n",
    "        ch=ch\n",
    "    else:\n",
    "        ch=ch+chr(k)\n",
    "print(ch)"
